{
    "name": "bcleverly/migration-unmake",
    "description": "Removes the last migration in your migrations folder",
    "type": "library",
    "homepage": "https://github.com/BCleverly/migration-unmake",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Cleverly",
            "email": "ben@bencleverly.dev",
            "homepage": "https://bencleverly.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "BCleverly\\MigrationUnmake\\MigrationUnmakeServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "BCleverly\\MigrationUnmake\\": "src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
